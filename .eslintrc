{
  "parserOptions": {
    "project": "./eslint/tsconfig.json"
  },
  "extends": [
    "./eslint/src/configs/all"
  ],
  "plugins": [
    "jest"
  ],
  "ignorePatterns": [
    "**/jest.config.ts",
    "**/jest-resolver.ts"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "@typescript-eslint/unbound-method": "off",
    "jest/unbound-method": "error"
  }
}
