{
  "name": "clean-code",
  "version": "0.1.0",
  "description": "clean code",
  "scripts": {
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goransh/clean-code.git"
  },
  "author": "Gøran Humlestøl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goransh/clean-code/issues"
  },
  "homepage": "https://github.com/goransh/clean-code#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-ghu": "file:eslint",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  }
}
